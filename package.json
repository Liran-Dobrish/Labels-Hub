    "react": "16.14.0",
    "react-dom": "16.14.0",
    "requirejs": "^2.3.7"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/react": "16.14.56",
    "@types/react-dom": "16.9.24",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "html-webpack-plugin": "^5.6.0",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "tfx-cli": "^0.21.3",
