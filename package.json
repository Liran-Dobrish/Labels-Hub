{
  "name": "labelshub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "webpack serve --mode development",
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --mode production",
    "build:dev": "npm run clean && webpack --mode development",
    "preview": "webpack serve --mode development --watch",
    "lint": "eslint . --ext .ts,.tsx",
